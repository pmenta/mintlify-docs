{
  "$schema": "https://mintlify.com/schema.json",
  "name": "JUDIT - Docs",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#15BE62",
    "light": "#0CAE56",
    "dark": "#15BE62"
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "topbarLinks": [
    {
      "name": "Swagger",
      "url": "https://api.judit.io/api"
    }
  ],
  "topbarCtaButton": {
