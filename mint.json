    "url": "https://app.judit.io"
  },
  "tabs": [
    {
      "name": "Referência da API",
      "url": "api-reference"
    }
  ],
  "navigation": [
    {
      "group": "Comece a usar",
      "pages": ["introduction", "quickstart"]
    },
    {
      "group": "Essenciais",
      "pages": [
        "essentials/requests",
        "essentials/tracking",
        "essentials/glossary"
      ]
    },
    {
      "group": "Endpoints de exemplo",
      "pages": [
        "api-reference/endpoint/get",
        "api-reference/endpoint/new-tracking",
        "api-reference/endpoint/find-trackings",
        "api-reference/endpoint/get-tracking",
        "api-reference/endpoint/deactivate-tracking",
        "api-reference/endpoint/find-attachment"
      ]
    }
  ],
  "primaryTab": {
    "name": "Documentação"
